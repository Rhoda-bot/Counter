.ytp-ad-preview-text { color: rgb(230, 230, 230); font-family: "Helvetica Neue Light", arial, sans-serif; font-size: 11px; padding: 10px 6px; text-shadow: rgba(0, 0, 0, 0.75) 0px 0px 4px; }

.ytp-ad-toggle-button:hover .ytp-ad-toggle-button-tooltip { display: none; }

.ytp-ad-instream-user-sentiment-dislike-button .ytp-ad-toggle-button-label, .ytp-ad-instream-user-sentiment-like-button .ytp-ad-toggle-button-label { padding: 7px 9px; }

.ytp-small-mode .ytp-ad-info-dialog-background .ytp-ad-info-dialog-form { max-width: 75%; font-size: 11px; }

.ytp-small-mode .ytp-ad-info-dialog-background .ytp-ad-button { right: 2px; top: 2px; }

.ytp-ad-confirm-dialog-cancel-button, .ytp-ad-confirm-dialog-confirm-button, .ytp-ad-info-dialog-confirm-button, .ytp-ad-feedback-dialog-confirm-button, .ytp-ad-feedback-dialog-cancel-button { padding: 0px !important; }

.ytp-small-mode .ytp-ad-confirm-dialog-cancel-button, .ytp-small-mode .ytp-ad-confirm-dialog-confirm-button, .ytp-small-mode .ytp-ad-info-dialog-confirm-button, .ytp-small-mode .ytp-ad-feedback-dialog-confirm-button, .ytp-small-mode .ytp-ad-feedback-dialog-cancel-button { font-size: 11px; }

.ytp-big-mode .ytp-ad-confirm-dialog-cancel-button, .ytp-big-mode .ytp-ad-confirm-dialog-confirm-button, .ytp-big-mode .ytp-ad-info-dialog-confirm-button, .ytp-big-mode .ytp-ad-feedback-dialog-confirm-button, .ytp-big-mode .ytp-ad-feedback-dialog-cancel-button { font-size: 14px; }

.ytp-big-mode .ytp-ad-confirm-dialog-close-overlay-button, .ytp-big-mode .ytp-ad-info-dialog-close-button, .ytp-big-mode .ytp-ad-feedback-dialog-close-button { height: 36px; width: 36px; }

.ytp-hide-controls .ytp-ad-instream-user-sentiment-container { bottom: 79px; }

.ytp-big-mode .ytp-ad-instream-user-sentiment-container { bottom: 145px; }

.ytp-ad-player-overlay-top-bar-gradients .ytp-ad-visit-advertiser-button::before { content: none; }

.ytp-ad-player-overlay-top-bar-gradients .ytp-ad-visit-advertiser-button { margin: auto 11px auto auto; pointer-events: auto; z-index: 33; }

.ytp-ad-player-overlay-top-bar-gradients .ytp-ad-visit-advertiser-button .ytp-ad-button-text { font-family: "Helvetica Neue Light", arial, sans-serif; font-size: 14px; line-height: normal; margin: auto 0px; padding: 0px 4px 5px; text-transform: capitalize; text-shadow: rgba(0, 0, 0, 0.75) 1px 1px 1px; color: rgb(255, 255, 255); text-decoration: none; }

.ytp-ad-player-overlay-top-bar-gradients .ytp-ad-visit-advertiser-button .ytp-ad-button-icon { display: none; }

.ytp-ad-player-overlay-top-bar-gradients .ytp-video-ad-top-bar-title { flex: 1 1 0%; font-family: arial, sans-serif; white-space: nowrap; color: rgb(255, 255, 255); text-overflow: ellipsis; text-shadow: rgba(0, 0, 0, 0.75) 1px 1px 1px; text-decoration: none; overflow: hidden; pointer-events: auto; cursor: pointer; font-size: 14px; max-width: 50%; height: 16px; line-height: normal; margin: 10px 5px 5px 10px; z-index: 33; opacity: 0.9; }

.ytp-ad-feedback-dialog-form input[type="radio"] { -webkit-appearance: radio; border: 1px solid; border-radius: 50%; background-color: rgba(0, 0, 0, 0.8); }

.ytp-ad-action-interstitial-background-container img:not([src]) { visibility: visible; }

.ytp-ad-action-interstitial-slot { background-color: rgba(50, 50, 50, 0.8); }

.ytp-ad-action-interstitial-headline, .ytp-ad-action-interstitial-description { color: rgb(255, 255, 255); }

#player { margin: 0px; top: 0px; }
